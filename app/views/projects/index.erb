<h1>All Projects: </h1>

<%@projects.each do |project|%>
	<h3><%=project.name%> </h3>
	Site Address: <%=project.address%> <br>
	Scheduled Duration: <%=project.scheduled_duration%> <br>
	

	<a href="/projects/<%=project.slug%>" >View Project Details</a>
	<hr style="border-top: dotted 1px;" />


<%end%>